%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%          TRATAMIENTOS DE VARIABLES
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%          TECNICAS DE ANALISis
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\begin{frame}{Metodología}
\textbf{Tecnicas de analisis}\\
\begin{itemize}
    \item Prueba multivariante de normalidad de Doornik-Hansen 
        \begin{equation}
            DH=\sum_{j=1}^{p}{({z_{1j}}^2+{z_{2j}}^2)}
        \end{equation}
    Donde $Z_{1j}$: coeficiente de asimetría de la muestra j y $Z_{2j}$: coeficiente de Curtosis de la muestra j, donde DH se distribuye como una $\chi^2.$\\
    \item Prueba no paramétrica de Kruskal-Wallis 
        \begin{equation}
            H=\frac{12}{N(N+1)}\sum_{i=1}^{k}{\frac{{R_i}^2}{n_i}-3(N+1)}
        \end{equation}
    Donde $n_j$: es el número de elementos de la muestra j,  $R_j$: suma de rangos de todos los elementos de la muestra j, $K$: número de muestras y $N$: número total de observaciones.
\end{itemize}
\end{frame}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%          TECNICAS DE ANALISis
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\begin{frame}{Metodología}
\textbf{Tecnicas de analisis}\\
\begin{itemize}
    \item Regresion Cuantilica 
        \begin{equation}
        {Sal}_{q50}=b_0+b_1Esc+b_1Exper+b_1{Exper}^2+b_1Hrs_sem+U        \end{equation}
Donde ${Sal}_{q50}$: Mediana de los salarios, $b_0$: Salario ceteris paribus, $b_i$: Efecto de la variable independiente sobre los salarios, $Esc$: Escolaridad, $Exper$: Experiencia laboral, ${Exper}^2$: Experiencia laboral al cuadrado (Obsolescencia de la experiencia laboral), $Hrs\_sem$: Horas trabajadas en el último mes y $U$: Término estocástico.\\
    
    \item Optimización de la función  
        \begin{equation}
           \frac{\partial {Salarios}_{q50}}{\partial Experiencia}=0
        \end{equation}
\end{itemize}
\end{frame}


